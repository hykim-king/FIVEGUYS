<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fiveguys.pilldex">
    <select id="doSelectOne"
            parameterType="com.fiveguys.pilldex.domain.UserVO" 
            resultType="com.fiveguys.pilldex.domain.UserVO" >
        SELECT *
        FROM member
        WHERE id = #{id} 
        AND pw = #{pw}
    </select>
</mapper>